        <rdfs:domain rdf:resource="http://www.semanticweb.org/project-entice/ontologies/2015/7/knowledgebase#Quality"/>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:onDatatype rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
                <owl:withRestrictions rdf:parseType="Collection">
                    <rdf:Description>
                        <xsd:minInclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#int">0</xsd:minInclusive>
                    </rdf:Description>
                    <rdf:Description>
                        <xsd:maxInclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#int">100</xsd:maxInclusive>
                    </rdf:Description>
                </owl:withRestrictions>
            </rdfs:Datatype>
        </rdfs:range>
        <rationale rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The developer could decide on an optimisation termination condition which might limit the possible size reduction.</rationale>
    </owl:DatatypeProperty>
    


