    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/project-entice/ontologies/2015/7/knowledgebase#Implementation_Algorithm -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/project-entice/ontologies/2015/7/knowledgebase#Implementation_Algorithm">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/project-entice/ontologies/2015/7/knowledgebase#Implementation_Algorithm"/>
