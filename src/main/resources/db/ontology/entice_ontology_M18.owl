        <rationale rdf:datatype="http://www.w3.org/2001/XMLSchema#string">This is a freestyle description of the output data of the disk image when it is running.</rationale>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/project-entice/ontologies/2015/7/knowledgebase#Functionality_Tag -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/project-entice/ontologies/2015/7/knowledgebase#Functionality_Tag">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/project-entice/ontologies/2015/7/knowledgebase#Functionality_Tag"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/project-entice/ontologies/2015/7/knowledgebase#Functionality"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rationale rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Tags may additionally be used to describe a functionality. By using tags, it may be possible to induce various functionality domains for which the users frequently upload or search for VMIs/Dis.</rationale>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/project-entice/ontologies/2015/7/knowledgebase#GeoLocation_DataProperty -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/project-entice/ontologies/2015/7/knowledgebase#GeoLocation_DataProperty">
