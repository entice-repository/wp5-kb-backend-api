    <!-- http://www.semanticweb.org/project-entice/ontologies/2015/7/knowledgebase#Implementation_DataProperty -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/project-entice/ontologies/2015/7/knowledgebase#Implementation_DataProperty">
