        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rationale rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Similarly to practices adopted by YouTube, functionalities must have clearly defined names corresponding to the core aspects covered.</rationale>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/project-entice/ontologies/2015/7/knowledgebase#Functionality_OutputDescription -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/project-entice/ontologies/2015/7/knowledgebase#Functionality_OutputDescription">
