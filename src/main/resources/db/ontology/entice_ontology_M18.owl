        <rdfs:domain rdf:resource="http://www.semanticweb.org/project-entice/ontologies/2015/7/knowledgebase#Functionality"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
        <rationale rdf:datatype="http://www.w3.org/2001/XMLSchema#string">This is a description of the programming language, algorithm, libraries used to implement the functionality, and may be used as part of the search criteria.</rationale>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">What is implementation_id?</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/project-entice/ontologies/2015/7/knowledgebase#Functionality_InputDescription -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/project-entice/ontologies/2015/7/knowledgebase#Functionality_InputDescription">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/project-entice/ontologies/2015/7/knowledgebase#Functionality_InputDescription"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/project-entice/ontologies/2015/7/knowledgebase#Functionality"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rationale rdf:datatype="http://www.w3.org/2001/XMLSchema#string">This is a freestyle description of the input data needed by the image. E.g. a data disk that should be mounted to the image.</rationale>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/project-entice/ontologies/2015/7/knowledgebase#Functionality_Name -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/project-entice/ontologies/2015/7/knowledgebase#Functionality_Name">
